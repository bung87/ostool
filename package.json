{
    "name": "@bung87/ostool",
    "version": "0.0.8",
    "description": "open source project tool",
    "main": "dist/index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "watch": "lsc -wco dist src",
        "build": "lsc -co dist src && cp -r src/templates dist/",
        "prepublishOnly": "npm run clean && npm run build",
        "test": "lsc tests"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bung87/ostool.git"
    },
    "files": [
        "dist"
    ],
    "bin": {
        "ostool": "dist/cli"
    },
    "author": "bung87",
    "license": "MIT",
    "devDependencies": {
        "husky": "^4.2.5",
        "livescript": "^1.6.0",
        "pirates": "^4.0.1",
        "tmp": "^0.2.1"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "common-path-prefix": "^3.0.0",
        "gitignore-globs": "^0.1.1",
        "glob": "^7.1.6",
        "handlebars": "^4.7.6",
        "ini": "^1.3.5",
        "inquirer": "^7.2.0",
        "inquirer-search-list": "^1.2.6",
        "is-ci": "^2.0.0",
        "license": "^1.0.3",
        "lodash.merge": "^4.6.2",
        "minimatch": "^3.0.4",
        "rimraf": "^3.0.2",
        "universal-diff": "^2.0.2",
        "yargs": "^15.3.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test"
        }
    }
}
